<Window x:Class="WpfCadCon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCadCon"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnRun" Content="캐드 외부 실행" Width="100" Height="100" PreviewMouseLeftButtonUp="btnRun_PreviewMouseLeftButtonUp"/>
        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
            <Button x:Name="btnReg" Content="레지스트리 등록" HorizontalAlignment="Left" Width="100" Height="100" PreviewMouseLeftButtonUp="btnReg_PreviewMouseLeftButtonUp"/>
            <Button x:Name="btnReg2" Content="레지스트리 삭제" HorizontalAlignment="Left" Width="100" Height="100" PreviewMouseLeftButtonUp="btnReg2_PreviewMouseLeftButtonUp"/>
        </StackPanel>
        
    </Grid>
</Window>
